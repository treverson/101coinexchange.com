<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, minimum-scale=1.0, maximum-scale=1.0, user-scalable=no,shrink-to-fit=no"/>
    <meta name="HandheldFriendly" content="true">
    <meta name="keywords" content="coin,exchange,bitcoin,rate,exhangerate,ethereum,ripple">
    <title>101 Coin Exchange</title>
    <link href="favicon.ico" rel="shortcut icon" type="image/x-icon" />

    <!-- Compiled and minified CSS -->
    <link rel="stylesheet" href="~/css/main.css">

    <!--Import Google Icon Font-->
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css?family=Montserrat" rel="stylesheet">

    <!-- Compiled and minified CSS -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.100.2/css/materialize.css">


    <script defer src="https://use.fontawesome.com/releases/v5.0.6/js/all.js"></script>

</head>
<body>
    <section class="top">
        <nav class="nav-extended">
            <div class="nav-wrapper black">
                <a href="#" class="brand-logo">&nbsp;</a>
                <div class="quotas">
                    <i class="fab fa-bitcoin"></i>&nbsp;=&nbsp;<a class="ico" id="icoBTC">...</a>
                    &nbsp;&nbsp;&nbsp;<i class="fab fa-cloudsmith"></i>&nbsp;=&nbsp;<a class="ico" id="icoXRP">...</a>
                    
                    <br>
                    &nbsp;<i class="fab fa-ethereum"></i>&nbsp;&nbsp;=&nbsp;<a class="ico" id="icoETH">...</a>
                </div>
                <a href="#" data-activates="mobile-demo" class="button-collapse"><i class="material-icons">menu</i></a>
                <ul id="nav-mobile" class="right hide-on-med-and-down">
                    <li><a href="/About">About</a></li>
                    <li><a href="#">FAQ</a></li>
                    <li><a href="#">Contact</a></li>
                </ul>

                <ul class="side-nav" id="mobile-demo">
                    <li><a href="#">About</a></li>
                    <li><a href="#">FAQ</a></li>
                    <li><a href="#">Contact</a></li>
                </ul>
            </div>

        </nav>
    </section>

    @RenderBody()

@*
    <hr />
    <footer>
        <p>&copy; @DateTime.Now.Year - Coiner</p>
    </footer>
*@
</body>
</html>






